{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "functions": {
    "app/api/workflows/route.ts": {
      "maxDuration": 10
    },
    "app/api/artifacts/[runId]/route.ts": {
      "maxDuration": 10
    },
    "app/api/download/[artifactId]/route.ts": {
      "maxDuration": 10
    },
    "app/api/branches/route.ts": {
      "maxDuration": 10
    },
    "app/api/pulls/route.ts": {
      "maxDuration": 10
    }
  }
}